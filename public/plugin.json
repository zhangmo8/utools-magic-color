{
  "main": "index.html",
  "preload": "preload/services.js",
  "logo": "logo.png",
  "development": {
    "main": "http://localhost:5173"
  },
  "features": [
    {
      "code": "color",
      "explain": "颜色匹配",
      "cmds": [
        "color"
      ]
    },
    {
      "code": "regex",
      "cmds": [
        {
          "type": "regex",
          "label": "匹配颜色",
          "match": "/^(#(?:[0-9a-fA-F]{3,8})|0x[0-9a-fA-F]{6,8}|rgb\\(\\s*([0-9]{1,3}\\s*,\\s*){2}[0-9]{1,3}\\s*\\)|rgba\\(\\s*([0-9]{1,3}\\s*,\\s*){3}(0|1|0?\\.\\d+)\\s*\\))$/i",
          "minLength": 1,
          "maxLength": 1000
        }
      ]
    }
  ]
}
